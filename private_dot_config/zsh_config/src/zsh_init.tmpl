        PROMPT=' %F{magenta}%n%f%F{green}@%m%f %F{green}${vcs_info_msg_0_}%f%F{blue}%2~%f %F{yellow}->%f '
    else
        PROMPT=' %F{green}@%m%f %F{green}${vcs_info_msg_0_}%f%F{blue}%2~%f %F{yellow}->%f '
    fi
}
